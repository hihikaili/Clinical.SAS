%datasets_MixedNoSide(ds=sm_v, evtest=Skin smoothness, site=4, grp=%str(and EVGRPID='VISUAL'), order=2.5);
%datasets_MixedNoSide(ds=even, evtest=Skin tone evenness, site=4, order=2.6);
