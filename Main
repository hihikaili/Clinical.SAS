/*** clear work library ***/
proc datasets lib=work memtype=data kill; quit;

/*** set datapath, outputpath and project***/
%let datapath=%str(G:\Scientific Computing\12 - Advanced Research\SMAD\Data\ACR_SMAD_18-12664_DB_lock_20190711);
%let outputpath=%str(G:\Scientific Computing\12 - Advanced Research\SMAD\Outputs);
%let project=SMAD;
libname &project "&datapath";

/*** import format if any ***/
proc format cntlin=&project..formats;
run;

proc format;
  value $trt "Group 1"="Placebo"
             "Group 2"="Neutrogena Regenerating Cream"
			 'Group 3'='0.15%SM 5253'
			 ;
  value trtn 1="Placebo"
             2="Neutrogena Regenerating Cream"
			 3='0.15%SM 5253'
			 ;
run;

options fmtsearch=(work &project);
ods graphics / noborder;


data attrmap;
  length value $50 fillcolor $20 markersymbol $20 markercolor $20 linecolor $20;
  input value $1-30 fillcolor $ markersymbol $ markercolor $ linecolor $ linepattern id $;
datalines;
Placebo                       brown circlefilled brown brown 1 myid1
Neutrogena Regenerating Cream lightblue circlefilled lightblue lightblue 1 myid1
0.15%SM 5253                  moderateblue circlefilled moderateblue moderateblue 1 myid1
;
run;

/************************************************************************************/

data adeff;
  set &project..eval;
  test=catx("-",put(cat, $cat.), put(scat, $scat.), evtest, evgrpid, "Site: "||put(site, site.));
  time=put(visitdy, timenum.);
run;

proc sort data=adeff;
  by randoid evtest visitdy tpt tptref;
run;

proc sort data=&project..dbext_rd;
  by randoid;
run;

data adeff;
  merge adeff &project..dbext_rd(keep=randoid rdstresc);
  by randoid;
  treatment=rdstresc;
  trt=put(treatment, $trt.);
  if treatment="Group 1" then trtn=1;
    else if treatment="Group 2" then trtn=2;
	else if treatment="Group 3" then trtn=3;
  if randoid not in (., 16, 19, 43, 48, 52, 65, 76, 79, 81, 91, 96, 98, 109, 111, 115, 118, 137);
  if evunused ne 1; 
run;

proc freq data=adeff;
  tables test*side;
run;


proc sort data=adeff;
  by test evtest trtn trt randoid visitdy;
run;



