%datasets_MixedNoSide(ds=Sting, evtest=Stinging, site=4, order=4.4);
%datasets_MixedNoSide(ds=tight, evtest=Tightness, site=4, order=4.5);
%datasets_MixedNoSide(ds=Tingl, evtest=Tingling, site=4, order=4.6);


proc sql;
  create table summary as
  select * from diffs_trt_dry union
  select * from diffs_trt_edema union 
  select * from diffs_trt_erythema union 
  select * from diffs_trt_peel union 
  select * from diffs_trt_wk_cf union 
  select * from diffs_trt_wk_ue union 
  select * from diffs_trt_wk_fh union 
  select * from diffs_trt_sm_t union 
  select * from diffs_trt_sm_v union 
  select * from diffs_trt_even union 
  select * from diffs_trt_corneo union 
  select * from diffs_trt_tewl union 
  select * from diffs_trt_burn union 
  select * from diffs_trt_itch union 
  select * from diffs_trt_sting union 
  select * from diffs_trt_tight union 
  select * from diffs_trt_tingl
  ;
quit;/*pain is not in the table*/

data summary;
  set summary;
  cat=substr(put(order, 3.), 1);
  wk="";
  mod="";
  str="";
run;


proc sort data=summary;
  by order trtn comp visitdy;
run;

/**********************************************************************************************/
/**********************************************************************************************/
ods escapechar="^";

proc format;
 
  value pcolor 0.1 - high = "Red"
               0.05 -< 0.1 = "Yellow"
			   low -< 0.05 = "Green"
			   ;
  value effcolor -0.5 - 0.5 = "Red"
                 0.5 <- 0.8 = "Orange"
				 -0.8 -< -0.5 = "Orange"
				 0.8 <- 1.5 = "Yellow"
				 -1.5 -< -0.8 = "Yellow"
				 1.5 <- 2 = "Lightgreen"
				 -2 -< -1.5 = "Lightgreen"
				 2 <- high = "Green"
				 low -< -2 = "Green"
                 ;
run;



options nodate;
ods graphics on / width=8in height=6in;
ods rtf file="&outputpath\SMAD_batch1_PPS.rtf" headery=320 startpage=off bodytitle;


proc report data=summary split="|" missing;
  format visitdy timenum. FalseDiscoveryRate pvalue7.4 eff 7.3;
  columns cat evtest comp visitdy ("Frequentist"(est ci FalseDiscoveryRate eff)) ("Bayesian"(wk mod str));
  *define order / order noprint;
  define cat / group noprint;
  define evtest / group "Endpoint" order=data style(column)=[just=left];
  define comp / display "Comparison" order=data style(column)=[just=left];
  define visitdy / display "Time" style(column)=[just=center];
  define est / display "Estimate|(StdErr)" style(column)=[just=center];
  define ci / display "95% CI" style(column)=[just=center];
  define FalseDiscoveryRate / display "Adjusted|P*" style={background=pcolor. just=center};
  define eff / display "Effect|Size" style={background=effcolor. just=center};
  define wk / display "Weak|Effect" style(column)=[just=center];
  define mod / display "Moderate|Effect" style(column)=[just=center];
  define str / display "Strong|Effect" style(column)=[just=center];
  compute before cat;
    line "";
	endcomp;
  compute after evtest;
    line "";
	endcomp;
title "Summary Table";
run;
title;

